<% @fleets.each do |fleet| %>
  <%= simple_form_for fleet, :url => url_for(:controller => 'generic_fleets', :action => 'results') do |f| %>
    <%= fleet.generic_unit.name %>
    <%= f.input :quantity, :collection => 1..fleet.quantity %>
    <%= f.hidden_field :id %>
    <%= f.button :submit %>
  <% end %>
<% end %>
